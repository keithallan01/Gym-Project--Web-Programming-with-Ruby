<h1>Members</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.full_name %></a></td>
    <td><%= member.age %></td>
  </tr>
  <% end %>
</table>
<hr>
<a href="/members/new">Add a New Member</a>
<br>
<a href="/bookings">View all Bookings</a>
